<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="500.0" maxWidth="750.0" minHeight="-Infinity" minWidth="-Infinity" onMouseDragged="#dragged" onMousePressed="#pressed" prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.magyart.random.sample.Controller">
   <children>
      <HBox alignment="TOP_RIGHT" prefHeight="25.0" prefWidth="750.0">
         <children>
            <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#closeApp" styleClass="closeButton" stylesheets="@../CSS/style.css" text="X">
               <font>
                  <Font name="System Bold" size="10.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <TextField fx:id="namer" layoutX="301.0" layoutY="88.0" promptText="name" />
      <Button fx:id="gameStarter" layoutX="349.0" layoutY="173.0" mnemonicParsing="false" onAction="#startGame" text="Button" />
      <TextArea fx:id="asd" editable="false" focusTraversable="false" layoutX="537.0" layoutY="32.0" mouseTransparent="true" prefHeight="255.0" prefWidth="180.0" wrapText="true" />
      <Button layoutX="432.0" layoutY="305.0" mnemonicParsing="false" onAction="#ptakeDamage" text="pDamage" />
      <Button fx:id="healBtn" layoutX="432.0" layoutY="262.0" mnemonicParsing="false" onAction="#Heal" text="Heal" />
      <Button fx:id="attackBtn" layoutX="366.0" layoutY="262.0" mnemonicParsing="false" onAction="#attack" text="Attack" />
   </children>
</AnchorPane>
